{
    "name": "language-liquid",
    "displayName": "Liquid Languages Support",
    "description": "Liquid Languages Syntax Highlighter",
    "version": "0.0.2",
    "publisher": "neilding",
    "engines": {
        "vscode": "^1.0.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "liquid",
            "aliases": ["HTML Liquid", "liquid"],
            "extensions": [".liquid"],
            "configuration": "./liquid.configuration.json"
        }],
        "grammars": [{
            "language": "liquid",
            "scopeName": "text.html.liquid",
            "path": "./syntaxes/liquid.tmLanguage"
        }]
    }
}